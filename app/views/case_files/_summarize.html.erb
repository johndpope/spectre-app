<li>
  <table class="table summary">
    <tbody>
      <tr>
        <td>
          <%= render "case_profile", case_file: case_file, content: content %>
        </td>
        <td>
          <div class="well">
            <% if case_file.actions.first  %>
              <%= render "actions/action", action: case_file.actions.first %>
            <% else %>
              <strong>No Actions Found</strong>
            <% end %>
          </div>
        </td>
      </tr>
      <tr>
        <td></td>
        <td>
          <span class="pull-right">
            <%= link_to "View Case", case_file_path(case_file), class: "btn btn-primary btn-small" %>
          </span>
        </td>
      </tr>
    </tbody>
  </table>
</li>