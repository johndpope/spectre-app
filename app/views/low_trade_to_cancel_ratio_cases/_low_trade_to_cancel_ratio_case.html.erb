<% if locals[:summarize] %>
  <%= render "case_files/summarize", case_file: low_trade_to_cancel_ratio_case,
                                     content: JSON.parse(
                                       low_trade_to_cancel_ratio_case.content,
                                       symbolize_names: true) %>
<% else %>
  <div class = "span12">
    <script type="text/javascript"> $(function() {
      new Highcharts.Chart({
        chart: {
          renderTo: 'events_chart',
          backgroundColor: null,
          type: 'column',
          inverted: true
        },
        title: {
          style: {
            color: "#FFF"
          },
          text: 'Trade-to-Cancel Ratio'
        },
        plotOptions: {
          column: {
            stacking: 'normal'
          },
          dataLabels: {
            enabled: true,
          }
        },
        credits: {
          enabled: false
        },
        xAxis: {
          categories: [
            'Monitoring Period'
          ],
          labels: {
            style: {
              color: '#CCC',
            }
          }
        },
        yAxis: {
          lineWidth: 0,
          gridLineWidth: 0.5,
          gridLineDashStyle: 'dot',
          minorGridLineWidth: 0,
          lineColor: 'transparent',
          title: {
            text: "Number of events"
          },
          labels: {
            style: {
              color: '#FFF',
            }
          }
        },
        series: [{
          name: 'Cancels',
          data: [<%= content[:'cancels'] %>],
          stack: 'Events',
          color: '#ff3333'
        },
        {
          name: 'Trades',
          data: [<%= content[:'fills'] %>],
          stack: 'Events',
          color: '#86c440'
        }],
      })
    });</script>
  <div id="events_chart" style="width:940px; height:425px;"></div>
  </div>
  <div class = "span12">
    <% if content %>
      <table class="table table-bordered table-striped table-hover">
        <thead>
          <tr>
            <th>Symbol</th>
            <th>Trades</th>
            <th>Cancels</th>
            <th>Ratio</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= content[:'symbol'] %></td>
            <td><%= content[:'fills'] %></td>
            <td><%= content[:'cancels'] %></td>
            <td><%= content[:'ratio'].to_f * 100 %>%</td>
          </tr>
        </tbody>
      </table>
    <% else %>
      <div class="well-small">
        <span style="color:red">No Events Found</span>
      </div>
    <% end %>
  </div>
  <div class = "span12" style="height:25px;"></div>
<% end %>